{
  "name": "Cop Bot",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "manifest_version": 2,
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
